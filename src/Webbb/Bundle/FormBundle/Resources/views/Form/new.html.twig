{% extends 'WebbbFormBundle::layout.html.twig' %}

{% block webbbadmin_main_content -%}
    <h1>Form creation</h1>

    <form action="{{ path('form_create') }}" method="post" {{ form_enctype(form) }}>
        
        {% include 'WebbbFormBundle:Form/modules/form:form-inner.html.twig' with {'form': form} %}
        
        <p>
            <button type="submit">Create</button>
        </p>
    </form>

        <ul class="record_actions">
    <li>
        <a href="{{ path('form') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
